{
  "name": "@team-plain/typescript-sdk",
  "version": "0.2.0",
  "description": "Typescript SDK for Plain's Core GraphQL API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "build:docs": "typedoc --plugin typedoc-plugin-missing-exports src/index.ts",
    "codegen": "graphql-codegen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts'",
    "test": "vitest",
    "changeset": "changeset",
    "release": "npm whoami && npm run build && changeset publish"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/typed-document-node": "^4.0.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-document-nodes": "^3.0.4",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "esbuild": "^0.17.18",
    "eslint": "^8.40.0",
    "nock": "^13.3.1",
    "rollup": "^3.21.5",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typedoc": "^0.24.7",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "axios": "^1.4.0",
    "graphql": "^16.6.0",
    "zod": "^3.21.4"
  }
}
